<div class="main-title-box">
  <div class = "">
      <img src="images/download_text.png" alt="main_text" class="page_title_text" />
  </div>
</div>

{{> navbar }}

<div class="context-box">
  <h4 class="title-content">Your Receipt</h4>
</div>

{{#if orderId}}
<div class="downloads-box">
    <p class="downloads-name title-content">  {{ this.orderId }}</p></br>
        <p class="downloads-name title-content">  {{ this.orderprice }}</p></br>
</div>
{{/if}}


{{#each receiptDetails}}
<div class="downloads-box">
    <p class="downloads-name title-content">  {{ this.name }}</p></br>
    <p class="downloads-price">{{ this.price }}</p></br>
    <img class="downloads-image" src = {{ this.image }} alt=" " /></br>
</div>
{{/each}}

{{> footer }}
